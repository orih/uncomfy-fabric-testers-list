{
  "name": "Uncomfy Fabric Testers List",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Why do testers and groups line same level?",
  "author": "orih",
  "icons": {
    "16" : "images/icon-16.png",
    "48" : "images/icon-48.png",
    "128" : "images/icon-128.png"
  },
  "permissions": [
    "tabs",
    "https://fabric.io/*/apps/*/beta/releases/*"
  ],
  "content_scripts": [{
      "matches": ["https://fabric.io/*/apps/*/beta/releases/*"],
      "js": ["js/jquery.min.js", "js/make-it-comfy.js"],
      "run_at": "document_start"
  }],
  "browser_action": {
    "default_icon": "images/icon-128.png",
    "default_title": "Made Fabric comfy :)"
  }
}